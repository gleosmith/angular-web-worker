{
    "extends": "./tsconfig.json",
    "angularCompilerOptions": {
      "skipTemplateCodegen": true,
      "strictMetadataEmit": true,
      "fullTemplateTypeCheck": true,
      "enableResourceInlining": true
    },
    "buildOnSave": false,
    "compileOnSave": false,
    "compilerOptions": {
      "baseUrl": ".",
      "target": "es2015",
      "module": "es2015",
      "outDir": "AUTOGENERATED",
      "declaration": true,
      "declarationDir": "AUTOGENERATED",
      "inlineSourceMap": true,
      "inlineSources": true,
      "skipLibCheck": true,
      "emitDecoratorMetadata": true,
      "experimentalDecorators": true,
      "importHelpers": true,
      "lib": ["dom", "es2018"],
      "paths": {
        "angular-web-worker": [
          "./dist"
        ],
        "angular-web-worker/*": [
          "./dist/*"
        ]
      }
    },
    "exclude": [ "schematics/*/**.*", "**/*.ngfactory.ts", "**/*.shim.ts", "**/*.spec.ts", "dist"],
    // "files": ["AUTOGENERATED"]
  }
  